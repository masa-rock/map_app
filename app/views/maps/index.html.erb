<section class="top-container-img">
  <% @post.each do |post| %>
    <div class="map_items">
      <%= post.name %>
      <% post.tags.each do |tag| %>
        <%= tag.name %>
      <% end %>
    </div>
  <% end %>
  <%= link_to '新規登録', new_page_path %>
  <%= link_to '一覧ページ', maps_index_path %>
</section>
